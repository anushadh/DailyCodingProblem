package problems;

public class Fibonacci {
	
	public static void main(String args[]) {
	/*	final long startTime = System.currentTimeMillis();
		int n = 999999999, i = 0;
		for(int i = 0; i < n; i++) {
			sum += i;
		}
		final long endTime = System.currentTimeMillis();
		System.out.println(sum);
