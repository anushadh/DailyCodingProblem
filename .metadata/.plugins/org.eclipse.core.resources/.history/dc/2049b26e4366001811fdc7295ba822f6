		System.out.println("---------------------------");
		final long startTime = System.currentTimeMillis();
		int n = 900000000, sum=0, i = 0;
		while(i < n) {
			sum += i;
			i++;
		}
		final long endTime = System.currentTimeMillis();
		System.out.println(sum);
		System.out.println(endTime + " " + startTime);
	}

}
